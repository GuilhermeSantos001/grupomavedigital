  "scripts": {
    "installer": "npm i --global gulp-cli && npm i --global npx yarn && yarn install --audit",
    "update": "npx yarn upgrade-interactive --latest",
    "dev": "npx ts-node-dev --inspect=9329 --respawn --watch 'ts/**' --transpile-only --ignore-watch ./node_modules -r tsconfig-paths/register ./ts/bin/www.ts",
    "dev-gql": "npx ts-node-dev --inspect=9239 --respawn --watch 'ts/**' --transpile-only --ignore-watch ./node_modules -r tsconfig-paths/register ./ts/graphql/index.ts",
    "clean": "rimraf dist/",
    "copy-files": "npx copyfiles -u 1 ts/**/*.gql dist/",
    "build": "npx yarn clean && npx yarn test && tsc --project ./tsconfig.json && tscpaths -p ./tsconfig.json -s ./ts -o ./dist && npx yarn copy-files",
    "start": "node --max-old-space-size=2048 -r dotenv/config ./dist/bin/www",
    "start-gql": "node --max-old-space-size=2048 -r dotenv/config ./dist/graphql/index",
    "test": "jest --watch",
    "test:ci": "jest --ci"
  },
