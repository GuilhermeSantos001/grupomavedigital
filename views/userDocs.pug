  script(src="/javascripts/lz-string.min.js")
  script.
    $(document).ready(() => {})

    var readAll_1 = false,
      readAll_2 = false;

    $("#modal-1-scrollable").on("scroll", function() {
      if (readAll_1) return;

      readAll_1 = this.scrollHeight - this.scrollTop === this.clientHeight;

      if (readAll_1) {
        $("#modal-1-button")
          .addClass('btn-primary')
          .removeClass('btn-light');
        document.getElementById("modal-1-button").disabled = false;
      }
    });

    $("#modal-2-scrollable").on("scroll", function() {
      if (readAll_2) return;

      readAll_2 = this.scrollHeight - this.scrollTop === this.clientHeight;

      if (readAll_2) {
        $("#modal-2-button")
          .addClass('btn-primary')
          .removeClass('btn-light');
        document.getElementById("modal-2-button").disabled = false;
      }
    });

    function gotoSystem() {
      document.location = `${baseurl}/system?usr_token=${localStorage.getItem('usr-token')}&usr_internetadress=${localStorage.getItem('usr-internetadress')}`;
    }

    function docInputChange(inputID, buttonID) {
      if (document.getElementById(inputID).files.length > 0) {
        document.getElementById(buttonID).disabled = false;
      } else {
        document.getElementById(buttonID).disabled = true;
      }
    }

    function printerDoc(docName) {
      alerting('O download iniciara em breve...');
      window.open(
        `${baseurl}/user/docs/print?authorization=${String("#{authorization}")}&docname=${String(docName)}&name=${String("#{name} #{surname}")}&street=${String("#{street}")}&number=${String("#{number}")}&district=${String("#{district}")}&state=${String("#{state}")}&city=${String("#{city}")}&zipcode=${String("#{zipcode}")}&cnpj=${String("#{cnpj}")}&day=${String("#{day}")}&month=${String("#{month}")}&year=${String("#{year}")}&valor=${String("#{valor}")}&usr_token=${localStorage.getItem('usr-token')}&usr_internetadress=${localStorage.getItem('usr-internetadress')}`,
        "_blank"
      );
    }

    function uploadDoc(docInputID, docButtonID, docname) {
      let formData = new FormData(),
        docFile = document.getElementById(`${docInputID}`);

      formData.append("attachment", docFile.files[0]);

      loading(true);

      axios.request({
        method: 'POST',
        url: `${baseurl}/user/docs/upload`,
        headers: {
          "Content-Type": "multipart/form-data",
          "usr_token": localStorage.getItem('usr-token'),
          "usr_internetadress": localStorage.getItem('usr-internetadress'),
          "authorization": "#{authorization}",
          "docname": docname
        },
        data: formData
      })
      .then(() => document.location.reload())
      .catch(error => {
        alerting(`Ocorreu um erro inesperado, fale com o administrador do sistema.`);
        console.log(error);
        loading(false);
      })
    }