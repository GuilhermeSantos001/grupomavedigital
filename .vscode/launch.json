{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "name": "Application",
            "request": "attach",
            "protocol": "inspector",
            "port": 9329,
            "restart": true,
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "name": "GraphQL",
            "request": "attach",
            "protocol": "inspector",
            "port": 9239,
            "restart": true,
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "pwa-chrome",
            "url": "http://192.168.0.121:3000",
            "webRoot": "${workspaceFolder}"
        }
    ]
